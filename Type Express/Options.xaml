                                    <TextBlock x:Name="HomeBtnTxt_Copy2" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource GnLockCkStr}"/>
                                </StackPanel>
                            </Button>
                            <TextBlock x:Name="PasswordLbl" Padding="30,8,0,0" Text="{DynamicResource GnPasswordStr}"/>
