                            <TextBlock Text="{DynamicResource DefFontStr}" FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top"/>
                            <!--<TextBox x:Name="FontStyleTxt" Margin="30,5,0,0" Width="80" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                            <TextBox.ContextMenu>
                                <ContextMenu Visibility="Collapsed"/>
                            </TextBox.ContextMenu>
                        </TextBox>
                        <ComboBox x:Name="FontStyleCombo" Margin="0,5,0,0" Width="17" ToolTip="{DynamicResource FontFaceStr}" BorderThickness="0,1,1,1"/>
                        <TextBox x:Name="FontSizeTxt" Margin="5,5,0,0" Width="35" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                            <TextBox.ContextMenu>
                                <ContextMenu Visibility="Collapsed"/>
                            </TextBox.ContextMenu>
                        </TextBox>
                        <ComboBox x:Name="FontSizeCombo" Margin="0,5,10,0" Width="17" BorderThickness="0,1,1,1" ToolTip="{DynamicResource FontSizeStr}"/>                    
                        <CheckBox x:Name="BoldCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpBoldStr}">
                            <Image x:Name="HelpBtnImg7" Source="Bold.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>
                        <CheckBox x:Name="ItalicCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpItalicStr}">
                            <Image x:Name="HelpBtnImg1" Source="Italic.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>
                        <CheckBox x:Name="UnderlineCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpUnderlineStr}">
                            <Image x:Name="HelpBtnImg2" Source="Underline.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>-->
                            <TextBox x:Name="FontStyleTxt" Margin="30,5,0,5" Width="100" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Template="{DynamicResource RoundedTextBox}">
                                <TextBox.ContextMenu>
                                    <ContextMenu Visibility="Collapsed"/>
                                </TextBox.ContextMenu>
                            </TextBox>
                            <Button x:Name="MoreFontsBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="2,4,0,5" Height="25" ToolTip="{DynamicResource FontFaceStr}">
                                <DockPanel Height="25">
                                    <ContentControl x:Name="HelpBtnImg209" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" />
                                </DockPanel>
                            </Button>
                            <TextBox x:Name="FontSizeTxt" Margin="5,5,0,5" Width="40" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Template="{DynamicResource RoundedTextBox}">
                                <TextBox.ContextMenu>
                                    <ContextMenu Visibility="Collapsed"/>
                                </TextBox.ContextMenu>
                            </TextBox>
                            <Button x:Name="FontSizesBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="2,4,4,5" Height="25" ToolTip="{DynamicResource FontSizeStr}">
                                <DockPanel Height="25">
                                    <ContentControl x:Name="HelpBtnImg57" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" />
                                </DockPanel>
                            </Button>
                            <DockPanel Margin="10,5,0,0">
                                <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal">
                                    <Rectangle x:Name="BoldSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                    <Rectangle x:Name="ItalicSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                    <Rectangle x:Name="UnderlineSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                </StackPanel>
                                <Button x:Name="BoldBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnBoldStr}">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="BoldBtnImg" Content="{DynamicResource BoldIcon}" HorizontalAlignment="Left" Margin="5,0,5,2" Width="20" Height="24" VerticalAlignment="Top" />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="ItalicBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnItalicStr}">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="HelpBtnImg8" Content="{DynamicResource ItalicIcon}" Width="20" HorizontalAlignment="Left" Margin="5,0,5,2" Height="24" />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="UnderlineBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnUnderlineStr}">
                                    <StackPanel Orientation="Horizontal" Margin="0,0,0,2">
                                        <ContentControl x:Name="UnderlineBtnImg" Content="{DynamicResource UnderlineIcon}" Width="20" HorizontalAlignment="Left" Margin="5,0" Height="24" />
                                    </StackPanel>
                                </Button>
                            </DockPanel>
                        </DockPanel>
                        <DockPanel x:Name="TextColourPnl">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefTextClrStr}"/>
                            <xctk:ColorPicker x:Name="ColourPicker" Height="24" ColorMode="ColorCanvas" UsingAlphaChannel="False" RecentColorsHeader="Recent Colours" StandardColorsHeader="{DynamicResource StandardColourStr}" VerticalAlignment="Top" ToolTip="{DynamicResource TextColourStr}" HorizontalAlignment="Left" Width="97" Margin="30,5,0,0" ShowTabHeaders="False" ShowAvailableColors="False" SelectedColor="Black" AdvancedTabHeader="{DynamicResource AdvancedStr}"/>
                            <Button x:Name="TextColourBtn" Width="30" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Right" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppBorderlessButton}" ToolTip="{DynamicResource SetClrStr}" Margin="5,5,0,0" Height="24" VerticalAlignment="Top">
                                <DockPanel Width="30" Height="24">
                                    <ContentControl x:Name="HelpBtnImg3" Content="{DynamicResource RightIcon}" HorizontalAlignment="Center" Margin="0" Height="18" />
                                </DockPanel>
                            </Button>
                            <TextBlock x:Name="ColourPickerTxt" TextWrapping="Wrap" Margin="10,8,0,5" Text="{Binding SelectedColorText, ElementName=ColourPicker}"/>
                        </DockPanel>
                        <DockPanel x:Name="SaveLocationPnl">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefSaveLocationStr}"/>
                            <Button x:Name="ChooseLocationBtn" Content="{DynamicResource ChooseStr}" Height="30" Margin="30,5,0,0" Style="{DynamicResource AppBorderlessButton}" VerticalAlignment="Top" Background="{DynamicResource BackColor}" FontSize="14" Width="100" HorizontalAlignment="Left"/>
