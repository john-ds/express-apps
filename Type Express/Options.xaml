                        </DockPanel>
                        <DockPanel x:Name="FileTypePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefFileTypeStr}"/>
                            <Button x:Name="RTFRadio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="30,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="RTFRadioImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1"/>
                                    <TextBlock x:Name="HomeBtnTxt_Copy2422" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource RTFStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="TXTRadio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="TXTRadioImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0"/>
                                    <TextBlock x:Name="HomeBtnTxt_Copy241" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource TXTStr}"/>
                                </StackPanel>
                            </Button>
                            <!--<RadioButton x:Name="RTFRadio" Content="{DynamicResource RTFStr}" Margin="30,5,0,0" IsChecked="True" GroupName="FileTypeRadios"/>
                        <RadioButton x:Name="TXTRadio" Content="{DynamicResource TXTStr}" Margin="30,5,0,0" GroupName="FileTypeRadios"/>-->
                        </DockPanel>
                        <DockPanel x:Name="ColourSchemePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefClrSchemeStr}"/>
                            <Button x:Name="ClrSchemeBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppBorderlessButton}" Margin="30,5,0,0" Height="24">
                                <DockPanel Height="24">
                                    <TextBlock x:Name="ClrSchemeLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,3,15,0" Height="21.31" Text="{DynamicResource BasicStr}" Padding="0"/>
                                    <ContentControl x:Name="HelpBtnImg200_Copy12" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Right" Margin="0,0,10,0" Height="24" DockPanel.Dock="Right" />
                                </DockPanel>
                            </Button>
                            <!--<ComboBox x:Name="ColourSchemeCombo" Margin="30,5,0,0" Width="120" SelectedIndex="0">
                            <ComboBoxItem Content="{DynamicResource BasicStr}"/>
                            <ComboBoxItem Content="{DynamicResource BlueStr}"/>
                            <ComboBoxItem Content="{DynamicResource GreenStr}"/>
                            <ComboBoxItem Content="{DynamicResource RedOrangeStr}"/>
                            <ComboBoxItem Content="{DynamicResource VioletStr}"/>
                            <ComboBoxItem Content="{DynamicResource OfficeStr}"/>
                            <ComboBoxItem Content="{DynamicResource GreyscaleStr}"/>
                        </ComboBox>-->
                        </DockPanel>
                        <DockPanel x:Name="SpellcheckPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefSpellStr}"/>
                            <Button x:Name="EnglishRadio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="30,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="EnglishRadioImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop222" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource EnglishStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="FrenchRadio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="FrenchRadioImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop21" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource FrenchStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="SpanishRadio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="SpanishRadioImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop22" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource SpanishStr}"/>
                                </StackPanel>
                            </Button>
                            <!--<RadioButton x:Name="EnglishRadio" Content="{DynamicResource EnglishStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,5,0,15" IsChecked="True" GroupName="SpellRadios"/>
                        <RadioButton x:Name="FrenchRadio" Content="{DynamicResource FrenchStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,5,0,0" IsChecked="False" GroupName="SpellRadios"/>
                        <RadioButton x:Name="SpanishRadio" Content="{DynamicResource SpanishStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,5,0,0" IsChecked="False" GroupName="SpellRadios"/>-->
                        </DockPanel>
                        <Label x:Name="MenuHeaderLbl_Copy" Height="50"  VerticalContentAlignment="Bottom" Margin="0" Padding="30,5,5,5" FontSize="20" Content="{DynamicResource GeneralStr}" Background="{x:Null}" Foreground="{DynamicResource TextColor}"/>
                        <DockPanel x:Name="InterfacePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnLangStr}"/>
                            <Button x:Name="EnglishRadio1" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="30,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="EnglishRadio1Img" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource EnglishUKStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="FrenchRadio1" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
