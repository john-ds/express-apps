                        <RadioButton x:Name="FrenchRadio1" Content="{DynamicResource FrenchFRStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,5,0,0" IsChecked="False" GroupName="LanguageRadios"/>-->
                        </DockPanel>
                        <DockPanel x:Name="SoundsPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnSoundsStr}"/>
                            <Button x:Name="SoundBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="SoundImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
