<Window x:Name="Options1" x:Class="Options"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" xmlns:System="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="{DynamicResource OptionsStr}" Background="{x:Null}" UseLayoutRounding="True" Height="500" Width="600" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" WindowStyle="None" AllowsTransparency="True">
    <Window.Resources>
        <Storyboard x:Key="EntranceStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="grid1">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard Storyboard="{StaticResource EntranceStoryboard}"/>
        </EventTrigger>
    </Window.Triggers>
    <Grid Name="grid1">
        <Grid x:Name="grid" Margin="10,0,10,10" Background="{DynamicResource BackColor}" RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Grid.RenderTransform>
            <Grid.Effect>
                <DropShadowEffect BlurRadius="10" Color="#FFD8D8D8" Opacity="0.6" Direction="270"/>
            </Grid.Effect>
        </Grid>
        <DockPanel Margin="10,0,10,10">
            <DockPanel Height="30" Margin="0" VerticalAlignment="Top" Background="{DynamicResource AppColor}" DockPanel.Dock="Top">
                <Button x:Name="CloseBtn" Width="30" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" HorizontalAlignment="Right" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" DockPanel.Dock="Right" Style="{DynamicResource TransHoverCloseButton}" Margin="0" ToolTip="{DynamicResource CloseStr}">
                    <DockPanel>
                        <ContentControl Width="16" HorizontalAlignment="Center" Margin="0" Height="16" Content="{DynamicResource CloseWhiteIcon}"/>
                    </DockPanel>
                </Button>
                <ContentControl DockPanel.Dock="Left" Content="{DynamicResource AppWhiteIcon}" Height="20" Margin="5,0,0,0"/>
                <Button x:Name="TitleBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Stretch" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource TransButton}" Height="30">
                    <DockPanel x:Name="stackPanel444" RenderTransformOrigin="0.5,0.5">
                        <TextBlock VerticalAlignment="Center" Margin="0" Text="{Binding Title, ElementName=Options1}" TextAlignment="Center" Foreground="White" TextTrimming="CharacterEllipsis" Padding="10,0"/>
                    </DockPanel>
                </Button>
            </DockPanel>
            <Grid Background="{DynamicResource SecondaryColor}">
                <ScrollViewer x:Name="Scroller" Margin="180,0,0,0">
                    <StackPanel x:Name="OptionStack" Margin="0" Background="{DynamicResource BackColor}">
                        <Label x:Name="MenuHeaderLbl" Height="50"  VerticalContentAlignment="Bottom" Margin="0" Padding="30,5,5,5" FontSize="20" Content="{DynamicResource DefaultStr}" Background="{x:Null}" Foreground="{DynamicResource TextColor}"/>
                        <DockPanel x:Name="FontPnl" LastChildFill="False">
                            <TextBlock Text="{DynamicResource DefFontStr}" FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top"/>
                            <!--<TextBox x:Name="FontStyleTxt" Margin="30,5,0,0" Width="80" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                            <TextBox.ContextMenu>
                                <ContextMenu Visibility="Collapsed"/>
                            </TextBox.ContextMenu>
                        </TextBox>
                        <ComboBox x:Name="FontStyleCombo" Margin="0,5,0,0" Width="17" ToolTip="{DynamicResource FontFaceStr}" BorderThickness="0,1,1,1"/>
                        <TextBox x:Name="FontSizeTxt" Margin="5,5,0,0" Width="35" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                            <TextBox.ContextMenu>
                                <ContextMenu Visibility="Collapsed"/>
                            </TextBox.ContextMenu>
                        </TextBox>
                        <ComboBox x:Name="FontSizeCombo" Margin="0,5,10,0" Width="17" BorderThickness="0,1,1,1" ToolTip="{DynamicResource FontSizeStr}"/>                    
                        <CheckBox x:Name="BoldCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpBoldStr}">
                            <Image x:Name="HelpBtnImg7" Source="Bold.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>
                        <CheckBox x:Name="ItalicCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpItalicStr}">
                            <Image x:Name="HelpBtnImg1" Source="Italic.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>
                        <CheckBox x:Name="UnderlineCheck" Height="20" Width="45" VerticalContentAlignment="Center" Margin="0,4,0,0" ToolTip="{DynamicResource OpUnderlineStr}">
                            <Image x:Name="HelpBtnImg2" Source="Underline.png" Width="20" HorizontalAlignment="Left" Margin="0" Height="20" />

                        </CheckBox>-->
                            <TextBox x:Name="FontStyleTxt" Margin="30,5,0,5" Width="100" UndoLimit="0" IsUndoEnabled="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Template="{DynamicResource RoundedTextBox}">
                                <TextBox.ContextMenu>
                                    <ContextMenu Visibility="Collapsed"/>
                                </TextBox.ContextMenu>
                            </TextBox>
                            <Button x:Name="MoreFontsBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="2,4,0,5" Height="25" ToolTip="{DynamicResource FontFaceStr}">
                                <DockPanel Height="25">
                                    <ContentControl x:Name="HelpBtnImg209" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" />
                                </DockPanel>
                            </Button>
                            <DockPanel Margin="10,5,0,0">
                                <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal">
                                    <Rectangle x:Name="BoldSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                    <Rectangle x:Name="ItalicSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                    <Rectangle x:Name="UnderlineSelector" Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" Width="30" HorizontalAlignment="Left" Visibility="Hidden"/>
                                </StackPanel>
                                <Button x:Name="BoldBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnBoldStr}">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="BoldBtnImg" Content="{DynamicResource BoldIcon}" HorizontalAlignment="Left" Margin="5,0,5,2" Width="20" Height="24" VerticalAlignment="Top" />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="ItalicBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnItalicStr}">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="HelpBtnImg8" Content="{DynamicResource ItalicIcon}" Width="20" HorizontalAlignment="Left" Margin="5,0,5,2" Height="24" />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="UnderlineBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource FnUnderlineStr}">
                                    <StackPanel Orientation="Horizontal" Margin="0,0,0,2">
                                        <ContentControl x:Name="UnderlineBtnImg" Content="{DynamicResource UnderlineIcon}" Width="20" HorizontalAlignment="Left" Margin="5,0" Height="24" />
                                    </StackPanel>
                                </Button>
                            </DockPanel>
                        </DockPanel>
                        <DockPanel x:Name="TextColourPnl">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefTextClrStr}"/>
                            <xctk:ColorPicker x:Name="ColourPicker" Height="24" ColorMode="ColorCanvas" UsingAlphaChannel="False" RecentColorsHeader="{DynamicResource RecentColourStr}" StandardColorsHeader="{DynamicResource StandardColourStr}" VerticalAlignment="Top" ToolTip="{DynamicResource TextColourStr}" HorizontalAlignment="Left" Width="97" Margin="30,5,0,0" ShowTabHeaders="False" ShowAvailableColors="False" SelectedColor="Black" AdvancedTabHeader="{DynamicResource AdvancedStr}"/>
                            <Button x:Name="TextColourBtn" Width="30" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Right" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppBorderlessButton}" ToolTip="{DynamicResource SetClrStr}" Margin="5,5,0,0" Height="24" VerticalAlignment="Top">
                                <DockPanel Width="30" Height="24">
                                    <ContentControl x:Name="HelpBtnImg3" Content="{DynamicResource RightIcon}" HorizontalAlignment="Center" Margin="0" Height="18" />
                                </DockPanel>
                            </Button>
                            <TextBlock x:Name="ColourPickerTxt" TextWrapping="Wrap" Margin="10,8,0,5" Text="{Binding SelectedColorText, ElementName=ColourPicker}"/>
                        </DockPanel>
                        <DockPanel x:Name="SaveLocationPnl">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource DefSaveLocationStr}"/>
                            <Button x:Name="ChooseLocationBtn" Content="{DynamicResource ChooseStr}" Height="30" Margin="30,5,0,0" Style="{DynamicResource AppBorderlessButton}" VerticalAlignment="Top" Background="{DynamicResource BackColor}" FontSize="14" Width="100" HorizontalAlignment="Left"/>
                            <TextBlock x:Name="SaveLocationTxt" TextWrapping="Wrap" Margin="10,11,0,5" Text="This PC" TextTrimming="CharacterEllipsis" Padding="0,0,10,0"/>
                        </DockPanel>
                        <DockPanel x:Name="TimingsPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource OpTimingsStr}"/>
                            <TextBlock Padding="30,8,0,0" Text="{DynamicResource OpTimingsInfoStr}"/>
                            <xctk:DoubleUpDown x:Name="TimingsUpDown" Height="24" Width="55" Margin="15,5,0,0" Minimum="0.5" Maximum="10" Text="10" DisplayDefaultValueOnEmptyText="True" DefaultValue="2" MaxLength="4" Value="2" Increment="1" Padding="0,0,5,0"/>
                            <TextBlock Padding="15,8,0,0" Text="{DynamicResource OpSecondsStr}"/>
                        </DockPanel>
                        <DockPanel x:Name="SlideSizePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource OpSizeStr}"/>
                            <Button x:Name="Size169Radio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="30,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="Size169RadioImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1"/>
                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="16:9"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="Size43Radio" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="Size43RadioImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0"/>
                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="4:3"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                        <Label x:Name="MenuHeaderLbl_Copy" Height="50"  VerticalContentAlignment="Bottom" Margin="0" Padding="30,5,5,5" FontSize="20" Content="{DynamicResource GeneralStr}" Background="{x:Null}" Foreground="{DynamicResource TextColor}"/>
                        <DockPanel x:Name="InterfacePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnLangStr}"/>
                            <Button x:Name="EnglishRadio1" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="30,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="EnglishRadio1Img" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource EnglishUKStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="FrenchRadio1" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="{DynamicResource BackColor}" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,5,0,0" Height="24" Tag="en">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="FrenchRadio1Img" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0"/>
                                    <TextBlock x:Name="HomeBtnTxt_Cop32" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource FrenchFRStr}"/>
                                </StackPanel>
                            </Button>
                            <!--<RadioButton x:Name="EnglishRadio1" Content="{DynamicResource EnglishUKStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,5,0,0" GroupName="LanguageRadios" IsChecked="True"/>
                        <RadioButton x:Name="FrenchRadio1" Content="{DynamicResource FrenchFRStr}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="15,5,0,0" IsChecked="False" GroupName="LanguageRadios"/>-->
                        </DockPanel>
                        <DockPanel x:Name="SoundsPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnSoundsStr}"/>
                            <Button x:Name="SoundBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="SoundImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy9" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Text="{DynamicResource GnSoundsCkStr}" Padding="5,0,0,0"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                        <DockPanel x:Name="PromptPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnCloseStr}"/>
                            <Button x:Name="SavePromptBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="PromptImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Text="{DynamicResource GnCloseCkStr}" Padding="5,0,0,0"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                        <DockPanel x:Name="SlideshowPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource OpSlideshowStr}"/>
                            <Button x:Name="SlideshowBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="SlideshowImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Text="{DynamicResource OpSlideshowInfoStr}" Padding="5,0,0,0"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                        <Label x:Name="MenuHeaderLbl_Copy1" Height="50"  VerticalContentAlignment="Bottom" Margin="0" Padding="30,5,5,5" FontSize="20" Content="{DynamicResource AppearanceStr}" Background="{x:Null}" Foreground="{DynamicResource TextColor}"/>
                        <DockPanel x:Name="ColourPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource AppThemeStr}"/>
                            <Button x:Name="DarkModeBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="DarkModeImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy14" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource DarkModeStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="AutoDarkModeBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="AutoDarkModeImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0" />
                                    <TextBlock x:Name="HomeBtnTxt_Cop1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource AutoDarkModeStr}"/>
                                </StackPanel>
                            </Button>
                            <DockPanel x:Name="AutoDarkPnl" Visibility="Collapsed">
                                <TextBlock x:Name="WordLbl23" Padding="60,8,0,0" Text="{DynamicResource FromStr}"/>
                                <Button x:Name="Dark1Combo" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="5,5,0,0" Height="24">
                                    <DockPanel Height="24">
                                        <TextBlock x:Name="Dark1Lbl" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,4,10,0" Height="21.31" Text="18:00" Padding="0"/>
                                        <ContentControl x:Name="HelpBtnImg20011" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Right" Margin="0,0,10,0" Height="24" DockPanel.Dock="Right" />
                                    </DockPanel>
                                </Button>
                                <TextBlock x:Name="WordLbl234" Padding="5,8,0,0" Text="{DynamicResource UntilStr}"/>
                                <Button x:Name="Dark2Combo" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="5,5,0,0" Height="24">
                                    <DockPanel Height="24">
                                        <TextBlock x:Name="Dark2Lbl" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,4,10,0" Height="21.31" Text="06:00" Padding="0"/>
                                        <ContentControl x:Name="HelpBtnImg20012" Content="{DynamicResource DownIcon}" Width="11.5" HorizontalAlignment="Right" Margin="0,0,10,0" Height="24" DockPanel.Dock="Right" />
                                    </DockPanel>
                                </Button>
                            </DockPanel>
                        </DockPanel>
                        <DockPanel x:Name="RecentsPnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource GnRecentStr}"/>
                            <TextBlock Padding="30,8,0,0" Text="{DynamicResource GnRecentInfoStr}"/>
                            <xctk:IntegerUpDown x:Name="RecentUpDown" Height="24" Width="55" Margin="15,5,0,0" Minimum="0" Maximum="30" Text="10" Value="10" Padding="0,0,5,0"/>
                            <TextBlock Padding="15,8,0,0" Text="{DynamicResource GnRecentInfo2Str}"/>
                        </DockPanel>
                        <DockPanel x:Name="SavePnl" LastChildFill="False">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource ApSaveStr}"/>
                            <Button x:Name="SaveBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="SaveImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource ApSaveCkStr}"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                        <Label x:Name="MenuHeaderLbl_Copy2" Height="50"  VerticalContentAlignment="Bottom" Margin="0" Padding="30,5,5,5" FontSize="20" Content="{DynamicResource StartupStr}" Background="{x:Null}" Foreground="{DynamicResource TextColor}"/>
                        <DockPanel x:Name="StartupPnl" LastChildFill="False" Margin="0,0,0,20">
                            <TextBlock FontSize="14" Padding="30,15,0,0" DockPanel.Dock="Top" Text="{DynamicResource StartupInfoStr}"/>
                            <Button x:Name="MenuBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="MenuImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource StMenuStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="NotificationBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="NotificationImg" Content="{DynamicResource TickIcon}" Width="20" Margin="5,0,0,0" Tag="1" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy6" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource StNotificationStr}"/>
                                </StackPanel>
                            </Button>
                            <Button x:Name="RecentBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Height="24" Margin="30,5,0,0" DockPanel.Dock="Top">
                                <StackPanel Orientation="Horizontal">
                                    <ContentControl x:Name="RecentImg" Content="{DynamicResource UntickIcon}" Width="20" Margin="5,0,0,0" Tag="0" />
                                    <TextBlock x:Name="HomeBtnTxt_Copy7" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,10,0" Height="21.31" Padding="5,0,0,0" Text="{DynamicResource StRecentStr}"/>
                                </StackPanel>
                            </Button>
                        </DockPanel>
                    </StackPanel>
                </ScrollViewer>
                <Grid Width="180" HorizontalAlignment="Left" Background="{DynamicResource SecondaryColor}">
                    <Grid.Effect>
                        <DropShadowEffect Direction="360" Opacity="0.1" Color="#FFC3C3C3"/>
                    </Grid.Effect>
