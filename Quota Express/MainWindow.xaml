<Window x:Name="Quota1" x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quota_Express"
        mc:Ignorable="d"
        Title="Quota Express" Background="{DynamicResource BackColor}" Height="620" Width="880" WindowStartupLocation="CenterScreen" MinWidth="520" MinHeight="350" WindowStyle="None">
    <Window.Resources>
        <Storyboard x:Key="SideStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="grid4">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="DocStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="grid1">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="MenuBtnsFadeIn">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="HomeBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ViewBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ExportBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="MenuBtnsFadeOut">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="HomeBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ViewBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ExportBtn">
                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="HomeStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="HomePnl">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ViewStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ViewPnl">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ExportStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="ExportPnl">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="HomeMnStoryboard">
            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="DocTabSelector">
                <EasingThicknessKeyFrame KeyTime="0:0:0.1" Value="65,0,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ViewMnStoryboard">
            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="DocTabSelector">
                <EasingThicknessKeyFrame KeyTime="0:0:0.1" Value="153,0,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ExportMnStoryboard">
            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="DocTabSelector">
                <EasingThicknessKeyFrame KeyTime="0:0:0.1" Value="241,0,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
        </Storyboard>

        <Storyboard x:Key="QuotaHoverOut">
            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="MenuIcon">
                <EasingThicknessKeyFrame KeyTime="0:0:0.1" Value="4,9,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.Opacity)" Storyboard.TargetName="rect835">
                <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="100"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="QuotaHoverIn">
            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Margin)" Storyboard.TargetName="MenuIcon">
                <EasingThicknessKeyFrame KeyTime="0:0:0.1" Value="5,11,0,0"/>
            </ThicknessAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.Opacity)" Storyboard.TargetName="rect835">
                <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="LoaderStartStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="LoadingGrid">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="LoaderEndStoryboard">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="LoadingGrid">
                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        
        <!-- Shortcuts -->
        <RoutedUICommand x:Key="AllCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="NavigateCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="ChartCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="RefreshCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="DriveCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="FolderCommand"></RoutedUICommand>
        <RoutedUICommand x:Key="HelpCommand"></RoutedUICommand>

    </Window.Resources>
    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource AllCommand}" Executed="SelectAllBtn_Click" />
        <CommandBinding Command="{StaticResource NavigateCommand}" Executed="NavigateBtn_Click" />
        <CommandBinding Command="{StaticResource ChartCommand}" Executed="ChartBtn_Click" />
        <CommandBinding Command="{StaticResource RefreshCommand}" Executed="RefreshBtn_Click" />
        <CommandBinding Command="{StaticResource DriveCommand}" Executed="DriveBtn_Click" />
        <CommandBinding Command="{StaticResource FolderCommand}" Executed="AnalysisBtn_Click" />
        <CommandBinding Command="{StaticResource HelpCommand}" Executed="HelpBtn_Click" />
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Key="A" Modifiers="Ctrl" Command="{StaticResource AllCommand}" />
        <KeyBinding Key="F" Modifiers="Ctrl" Command="{StaticResource NavigateCommand}" />
        <KeyBinding Key="H" Modifiers="Ctrl" Command="{StaticResource ChartCommand}" />
        <KeyBinding Key="R" Modifiers="Ctrl" Command="{StaticResource RefreshCommand}" />
        <KeyBinding Key="D" Modifiers="Ctrl" Command="{StaticResource DriveCommand}" />
        <KeyBinding Key="E" Modifiers="Ctrl" Command="{StaticResource FolderCommand}" />
        <KeyBinding Key="F1" Command="{StaticResource HelpCommand}" />
    </Window.InputBindings>

    <Grid>
        <Grid x:Name="grid1" Background="{DynamicResource BackColor}" Margin="0">
            <Grid Margin="0">
                <Grid Margin="0,72,0,30">
                    <DockPanel x:Name="MainDock">
                        <Grid x:Name="SideBarGrid" HorizontalAlignment="Right" Width="235" DockPanel.Dock="Right">
                            <Grid x:Name="grid4">
                                <ScrollViewer Margin="0,44,0,0" VerticalScrollBarVisibility="Auto">
                                    <StackPanel>
                                        <TextBlock x:Name="ErrorDriveTxt" Margin="15,10,10,0" FontSize="14" VerticalAlignment="Top" Text="{DynamicResource ErrorDriveStr}" TextWrapping="Wrap" Visibility="Collapsed"/>
                                        <local:CircularProgressBar x:Name="CircleProgress" Style="{DynamicResource circularProgressBar}" VerticalAlignment="Top" Margin="0,20,0,0"/>
                                        <StackPanel x:Name="FolderStack" UseLayoutRounding="True" Visibility="Collapsed">
                                            <TextBlock x:Name="SubtitleFolderTxt" Margin="10,10,10,0" FontSize="14" VerticalAlignment="Top" Text="{DynamicResource SelectionFolderStr}" TextAlignment="Center" TextWrapping="Wrap"/>
                                            <TextBlock x:Name="SelectionHeaderTxt" Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource SelectionSizeStr}" TextAlignment="Center" Visibility="Collapsed"/>
                                            <TextBlock x:Name="SelectionTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="0 MB" HorizontalAlignment="Center" Visibility="Collapsed"/>
                                            <TextBlock x:Name="CurrentHeaderTxt" Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource ThisFolderStr}" TextAlignment="Center"/>
                                            <TextBlock x:Name="TopBtnTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="Documents" HorizontalAlignment="Center"/>
                                            <TextBlock x:Name="SizeHeaderTxt" Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource TotalFolderSizeStr}" TextAlignment="Center"/>
                                            <TextBlock x:Name="TotalSizeTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="0 MB" HorizontalAlignment="Center"/>
                                            <Button x:Name="CopyDetailsBtn" HorizontalAlignment="Center" Height="31" Margin="0,20" Style="{DynamicResource AppBorderlessButton}" VerticalAlignment="Top" Background="{DynamicResource BackColor}" FontSize="14">
                                                <Button.ContextMenu>
                                                    <ContextMenu x:Name="TopMenu" FontSize="14" HasDropShadow="False" Background="White" BorderThickness="1,1,1,1" BorderBrush="#FFABADB3" Style="{DynamicResource SimpleContextMenu}">
                                                        <MenuItem x:Name="CopyPathTopBtn" Header="{DynamicResource QtCopyPathStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                                        <MenuItem x:Name="CopySizeTopBtn" Header="{DynamicResource QtCopySizeStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                                        <MenuItem x:Name="CopySizeBytesTopBtn" Header="{DynamicResource QtCopySizeBytesStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                                    </ContextMenu>
                                                </Button.ContextMenu>
                                                <DockPanel Margin="5,0">
                                                    <TextBlock x:Name="HomeBtnTxt_Copy12" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,0,5,0" Text="{DynamicResource CopyDetailsStr}" Padding="5,0,0,0"/>
                                                    <ContentControl x:Name="CompareBtnImg" Content="{DynamicResource DownIcon}" Width="12" HorizontalAlignment="Left" Margin="3,0" />
                                                </DockPanel>
                                            </Button>
                                        </StackPanel>
                                        <StackPanel x:Name="DriveStack" UseLayoutRounding="True">
                                            <TextBlock x:Name="DriveNameTxt" Margin="10,10,10,0" FontSize="15" VerticalAlignment="Top" Text="C:\" TextAlignment="Center" TextWrapping="Wrap"/>
                                            <TextBlock x:Name="SubtitleDriveTxt" Margin="10,5,10,0" FontSize="14" VerticalAlignment="Top" Text="{DynamicResource TotalUsageStr}" TextAlignment="Center" TextWrapping="Wrap"/>
                                            <TextBlock Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource SpaceTakenStr}" TextAlignment="Center"/>
                                            <TextBlock x:Name="DriveTakenTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="0 MB" HorizontalAlignment="Center"/>
                                            <TextBlock Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource RemainingStr}" TextAlignment="Center"/>
                                            <TextBlock x:Name="DriveRemainingTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="0 MB" HorizontalAlignment="Center"/>
                                            <TextBlock Margin="0,20,0,0" FontSize="14" VerticalAlignment="Top" TextTrimming="CharacterEllipsis" Text="{DynamicResource TotalDriveStr}" TextAlignment="Center"/>
                                            <TextBlock x:Name="DriveTotalTxt" Margin="10,0" FontSize="15" TextTrimming="CharacterEllipsis" Text="0 MB" HorizontalAlignment="Center"/>
                                            <Button x:Name="DriveMenuBtn" HorizontalAlignment="Center" Height="31" Margin="0,20" Style="{DynamicResource AppBorderlessButton}" VerticalAlignment="Top" Background="{DynamicResource BackColor}" FontSize="14">
                                                <DockPanel Margin="5,0">
                                                    <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,0,5,0" Text="{DynamicResource OtherDrivesStr}" Padding="5,0,0,0"/>
                                                    <ContentControl Content="{DynamicResource DownIcon}" Width="12" HorizontalAlignment="Left" Margin="3,0" />
                                                </DockPanel>
                                            </Button>
                                        </StackPanel>
                                    </StackPanel>
                                </ScrollViewer>
                            </Grid>
                            <DockPanel Height="44" Margin="0" VerticalAlignment="Top" Background="{DynamicResource BackColor}">
                                <Rectangle Fill="{DynamicResource AppColor}" Height="2" VerticalAlignment="Top" DockPanel.Dock="Bottom"/>
                                <Button x:Name="HideSideBarBtn" Width="30" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Right" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" DockPanel.Dock="Right" Style="{DynamicResource AppButton}" ToolTip="{DynamicResource ClosePaneStr}" Margin="0" Height="42" VerticalAlignment="Top">
                                    <StackPanel Orientation="Horizontal" Width="30" Height="42">
                                        <ContentControl x:Name="HelpBtnImg47" Content="{DynamicResource RightIcon}" Width="24" HorizontalAlignment="Left" Margin="3,0,0,0" Height="24" />
                                    </StackPanel>
                                </Button>
                                <TextBlock x:Name="SideHeaderLbl" Margin="15,0,0,0" FontSize="15" VerticalAlignment="Center" HorizontalAlignment="Left" Width="177.827" TextTrimming="CharacterEllipsis" Text="Quota Express"/>
                            </DockPanel>
                        </Grid>
                        <ScrollViewer x:Name="Scroller" Margin="0" HorizontalScrollBarVisibility="Disabled">
                            <StackPanel x:Name="FileStack" Background="{DynamicResource BackColor}" Margin="20">
                                <Button x:Name="ItemBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0" Height="30" Visibility="Collapsed">
                                    <Button.ContextMenu>
                                        <ContextMenu x:Name="ItemMenu" FontSize="14" HasDropShadow="False" Background="White" BorderThickness="1,1,1,1" BorderBrush="#FFABADB3" Style="{DynamicResource SimpleContextMenu}">
                                            <MenuItem x:Name="OpenBtn" Header="{DynamicResource QtOpenStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                            <MenuItem x:Name="CopyPathBtn" Header="{DynamicResource QtCopyPathStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                            <MenuItem x:Name="CopySizeBtn" Header="{DynamicResource QtCopySizeStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>
                                            <MenuItem x:Name="CopySizeBytesBtn" Header="{DynamicResource QtCopySizeBytesStr}" BorderThickness="0" Style="{DynamicResource SimpleMenuItem}" Padding="5"/>

                                        </ContextMenu>
                                    </Button.ContextMenu>
                                    <Grid>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="80*"/>
                                                <ColumnDefinition Width="20*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid x:Name="HighlighterGrid" Background="{DynamicResource AppHoverColor}"/>
                                            <Grid Grid.Column="1"></Grid>
                                        </Grid>
                                        <DockPanel Height="30">
                                            <ContentControl Content="{DynamicResource UntickIcon}" Width="24" HorizontalAlignment="Left" Margin="3,0" Height="24" />
                                            <ContentControl x:Name="HelpBtnImg1" Content="{DynamicResource FolderIcon}" Width="24" HorizontalAlignment="Left" Margin="3,0" Height="24" />
                                            <TextBlock x:Name="HomeBtnTxt_Copy1" VerticalAlignment="Center" FontSize="14" Margin="0,7.69,10,7" Height="21.31" Text="Documents" Padding="5,0,0,0" TextTrimming="CharacterEllipsis"/>
                                        </DockPanel> 
                                    </Grid>
                                    
                                </Button>
                            </StackPanel>
                        </ScrollViewer>
                    </DockPanel>                    
                    <Grid x:Name="LoadingGrid" Background="{DynamicResource BackColor}" Visibility="Collapsed">
                        <DockPanel MaxHeight="400" Margin="0,30">
                            <Button x:Name="CancelBtn" Content="{DynamicResource QtCancelStr}" Height="30" Margin="0,20,0,0" Style="{DynamicResource AppRoundedButton}" VerticalAlignment="Center" Background="{DynamicResource BackColor}" FontSize="14" Width="89" HorizontalAlignment="Center" DockPanel.Dock="Bottom"/>
                            <ProgressBar x:Name="LoadingProgress" Foreground="{DynamicResource AppColor}" VerticalAlignment="Center" Height="5" Margin="0,10,0,0" BorderBrush="{x:Null}" Width="300" BorderThickness="0" DockPanel.Dock="Bottom"/>
                            <TextBlock x:Name="CalculatingTxt" Text="{DynamicResource LoaderSubtitleStr}" HorizontalAlignment="Center" FontFamily="Segoe UI" FontSize="14" VerticalAlignment="Center" Margin="0,5,0,0" DockPanel.Dock="Bottom"/>
                            <TextBlock Text="{DynamicResource QtCalculatingStr}" HorizontalAlignment="Center" FontFamily="Segoe UI Light" FontSize="24" VerticalAlignment="Center" Margin="0" DockPanel.Dock="Bottom"/>
                            <Viewbox>
                                <ContentControl Content="{DynamicResource FileLoaderVector}"/>
                            </Viewbox>
                        </DockPanel>
                    </Grid>

                </Grid>

                <TabControl x:Name="DocTabs" Margin="0,4,0,0" Height="68" Padding="0" BorderThickness="0" VerticalAlignment="Top">
                    <TabControl.Effect>
                        <DropShadowEffect Direction="270" Color="Gray" Opacity="0.1" BlurRadius="10"/>
                    </TabControl.Effect>
                    <TabItem x:Name="HomeTab" Header="TabItem" Focusable="False">
                        <Grid x:Name="grid2" Background="{DynamicResource SecondaryColor}">
                            <ScrollViewer x:Name="HomeScrollViewer" Margin="60,0,40,0" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled">
                                <ScrollViewer.Resources>
                                    <Style TargetType="{x:Type ScrollBar}">
                                        <Style.Triggers>
                                            <Trigger Property="Orientation" Value="Horizontal">
                                                <Setter Property="Height" Value="10"/>
                                                <Setter Property="MinHeight" Value="0"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </ScrollViewer.Resources>
                                <StackPanel x:Name="HomePnl" Margin="12,0,0,0" Background="{DynamicResource SecondaryColor}" Orientation="Horizontal" CanHorizontallyScroll="True" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" HorizontalAlignment="Left">
                                    <Button x:Name="TopBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,4,0,0" Height="42" IsEnabled="False" ToolTip="{DynamicResource TopBtnStr}">
                                        <StackPanel Orientation="Horizontal" Height="36">
                                            <ContentControl x:Name="TopBtnImg" Content="{DynamicResource NoUndoIcon}" Width="22" HorizontalAlignment="Left" Margin="5,0" Height="24" />
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="NavigateBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,4,0,0" Height="42" ToolTip="{DynamicResource NavigateToolStr}">
                                        <StackPanel Orientation="Horizontal" Height="36">
                                            <ContentControl x:Name="HelpBtnImg25" Content="{DynamicResource OpenIcon}" Width="24" HorizontalAlignment="Left" Margin="6,0,3,0" Height="24" />
                                            <TextBlock x:Name="HomeBtnTxt_Copy2" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,7.69,5,7" Height="21.31" Text="{DynamicResource ChooseFolderStr}" Padding="5,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Separator Margin="0,24,0,19" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Center" VerticalAlignment="Center" Width="20">
                                        <Separator.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform/>
                                                <SkewTransform/>
                                                <RotateTransform Angle="90"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </Separator.RenderTransform>
                                    </Separator>
                                    <Button x:Name="AnalysisBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,4,0,0" Height="42" ToolTip="{DynamicResource FolderToolStr}">
                                        <StackPanel Orientation="Horizontal" Height="36">
                                            <ContentControl Content="{DynamicResource BarChartIcon}" Width="20" HorizontalAlignment="Left" Margin="6,0,3,0" Height="24" />
                                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,7.69,10,7" Height="21.31" Text="{DynamicResource FolderAnalysisStr}" Padding="5,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="DriveBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,4,0,0" Height="42" ToolTip="{DynamicResource DriveToolStr}">
                                        <StackPanel Orientation="Horizontal" Height="36">
                                            <ContentControl Content="{DynamicResource DriveIcon}" Width="24" HorizontalAlignment="Left" Margin="6,0,3,0" Height="24" />
                                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,7.69,5,7" Height="21.31" Text="{DynamicResource DriveAnalysisStr}" Padding="5,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                    <Separator Margin="0,24,0,19" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Center" VerticalAlignment="Center" Width="20">
                                        <Separator.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform/>
                                                <SkewTransform/>
                                                <RotateTransform Angle="90"/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </Separator.RenderTransform>
                                    </Separator>
                                    <Button x:Name="HighlightingBtn" VerticalAlignment="Top" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Left" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0,4,0,0" Height="42" ToolTip="{DynamicResource PercentageToolStr}">
                                        <StackPanel Orientation="Horizontal" Height="36">
                                            <ContentControl x:Name="HighlightingImg" Content="{DynamicResource UntickIcon}" Width="24" HorizontalAlignment="Left" Margin="3,0" Height="24" Tag="0" />
                                            <TextBlock x:Name="HomeBtnTxt_Copy9" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14" Margin="0,7.69,10,7" Height="21.31" Text="{DynamicResource PercentageStr}" Padding="5,0,0,0"/>
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </ScrollViewer>
                            <Grid x:Name="HomeScroll" HorizontalAlignment="Right" Background="{DynamicResource SecondaryColor}">
                                <Grid.Effect>
                                    <DropShadowEffect Direction="180" Color="#FFA0A0A0" ShadowDepth="2" Opacity="0.2"/>
                                </Grid.Effect>
                                <Button x:Name="HomeLeftBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Right" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" DockPanel.Dock="Right" Style="{DynamicResource AppButton}" Margin="0,0,20,0" Width="20">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="HelpBtnImg475" Content="{DynamicResource LeftIcon}" Margin="0" Height="24" Width="20" />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="HomeRightBtn" Padding="0" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" HorizontalAlignment="Right" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" Style="{DynamicResource AppButton}" Margin="0" Width="20">
                                    <StackPanel Orientation="Horizontal">
                                        <ContentControl x:Name="HelpBtnImg35" Content="{DynamicResource RightIcon}" Margin="0" Height="24" Width="20" />
                                    </StackPanel>
                                </Button>
                            </Grid>
                        </Grid>
                    </TabItem>
                    <TabItem x:Name="ViewTab" Header="TabItem" Focusable="False">
                        <Grid Background="{DynamicResource SecondaryColor}">
                            <ScrollViewer x:Name="ViewScrollViewer" Margin="60,0,40,0" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Disabled">
                                <ScrollViewer.Resources>
                                    <Style TargetType="{x:Type ScrollBar}">
                                        <Style.Triggers>
                                            <Trigger Property="Orientation" Value="Horizontal">
                                                <Setter Property="Height" Value="10"/>
                                                <Setter Property="MinHeight" Value="0"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
